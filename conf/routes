# Menu
POST     /menu                       menu.Controller.createOrUpdateMenu()
POST     /menu/random                menu.Controller.selectRandomMenu()
POST     /menu/view                  menu.Controller.createOrUpdateMenuViewSchema()

# User
POST     /user                       user.Controller.createOrUpdateUser()
POST     /user/view                  user.Controller.createOrUpdateUserViewSchema()

# Webpage
GET      /assets/*file               controllers.Assets.at(path="/public", file)
